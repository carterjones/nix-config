{
  "[dockerfile]": {"editor.formatOnType": true},
  "[go]": {"editor.formatOnType": true},
  "[groovy]": {"editor.formatOnType": true},
  "[markdown]": {"editor.formatOnType": true},
  "[shellscript]": {"editor.formatOnType": true},
  "[terraform]": {"editor.formatOnType": true, "editor.formatOnSave": true},
  "[toml]": {"editor.formatOnType": true},
  "[yaml]": {"editor.formatOnType": true},
  "editor.fontFamily": "Hack",
  "editor.rulers": [79, 99],
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "gitlens.codeLens.enabled": false,
  "keyboard.touchbar.enabled": false,
  "python.linting.flake8Enabled": true,
  "rewrap.wholeComment": false,
  "shellcheck.executablePath": "/usr/local/bin/shellcheck",
  "shellcheck.exclude": ["SC1090", "SC1091"],
  "telemetry.telemetryLevel": "off",
  "terraform.languageServer.enable": true,
  "todohighlight.include": ["**/*.js", "**/*.txt", "**/*.md", "**/*.html", "**/*.go", "**/*.sh"],
  "todohighlight.isCaseSensitive": true,
  "todohighlight.isEnable": true,
  "todohighlight.keywordsPattern": "[Oo]bviously|[Bb]asically|[Ss]imply|[Oo]f course|[Cc]learly|[Ee]veryone knows|[Ee]asy|[Ee]asily",
  "window.restoreWindows": "all",
  "window.zoomLevel": 0,
  "workbench.editorAssociations": {
    "git-rebase-todo": "default"
  },
  "workbench.startupEditor": "newUntitledFile",
  "workbench.colorTheme": "Monokai",
  "yaml.schemaStore.enable": false
}
